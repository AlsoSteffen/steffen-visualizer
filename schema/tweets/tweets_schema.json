{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A Tweet Object",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "maxItems": 1,
      "minimum": 1
    },
    "link": {
      "type": "string",
      "maxItems": 1
    },
    "content": {
      "type": "string",
      "maxItems": 1
    },
    "date": {
      "type": "number",
      "maxItems": 1
    },
    "retweets": {
      "type": "number",
      "maxItems": 1
    },
    "favorites": {
      "type": "number",
      "maxItems": 1
    },
    "mentions": {
      "type": "string",
      "maxItems": 1
    },
    "tags": {
      "type": "string",
      "maxItems": 1
    }
  }
}