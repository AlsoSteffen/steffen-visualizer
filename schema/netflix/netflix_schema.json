{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A Netflix Title",
  "type": "array",
  "properties": {
    "netflix": {
      "type": "object",
      "id": {
        "type": "number",
        "maxItems": 1,
        "minimum": 1
      },
      "title_type": {
        "type": "string",
        "maxItems": 1,
        "enum": [
          "Movie",
          "Series"
        ]
      },
      "title": {
        "type": "string",
        "maxItems": 1
      },
      "director": {
        "type": "string",
        "maxItems": 1
      },
      "cast": {
        "type": "string",
        "maxItems": 1
      },
      "country": {
        "type": "string",
        "maxItems": 1
      },
      "dateAdded": {
        "type": "number",
        "maxItems": 1
      },
      "releaseYear": {
        "type": "number",
        "maxItems": 1,
        "minimum": 1900,
        "maximum": 2020
      },
      "rating": {
        "type": "string",
        "enum": [
          "G",
          "NC-17",
          "NR",
          "PG",
          "PG-13",
          "R",
          "TV-14",
          "TV-G",
          "TV-MA",
          "TV-PG",
          "TV-Y7",
          "TV-Y7-FV"
        ],
        "maxItems": 1
      },
      "duration": {
        "type": "string",
        "maxItems": 1
      },
      "category": {
        "type": "string",
        "maxItems": 1
      },
      "description": {
        "type": "string",
        "maxItems": 1
      }
    }
  }
}